        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "https://dingde.vip/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "timor",
            "email": "904981950@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "topthink/framework": "^6.0.0",
        "topthink/think-orm": "^2.0",
        "topthink/think-view": "^1.0",
        "topthink/think-multi-app": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "topthink/think-trace":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
